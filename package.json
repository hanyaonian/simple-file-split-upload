{
  "name": "file-upload-demo",
  "version": "1.0.0",
  "description": "a simple file upload demo",
  "main": "index.js",
  "scripts": {
    "dev": "parcel web/index.html",
    "serve": "nodemon serve/index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "web/*"
    ],
    "execMap": {
      "ts": "esno"
    }
  },
  "keywords": [
    "file-split",
    "large file upload",
    "File stream"
  ],
  "author": "yaonianhan",
  "license": "ISC",
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/node": "^18.0.0",
    "esno": "^0.16.3",
    "nodemon": "^2.0.16",
    "parcel": "^2.6.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "install": "^0.13.0",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-cors": "^0.0.16",
    "m-web-logger": "^0.1.0"
  }
}
